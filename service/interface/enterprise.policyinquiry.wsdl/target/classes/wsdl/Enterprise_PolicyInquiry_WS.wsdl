               <xs:element minOccurs="0" name="DistributionType" type="xs:string">
                  <xs:annotation>
                     <xs:documentation>
The distribution type (IAG reference)
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="DistributionGroup" type="xs:string">
                  <xs:annotation>
                     <xs:documentation>
The distribution group (IAG reference)
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
      <xs:complexType name="VehicleComponent">
      <xs:annotation>
         <xs:documentation>
Describes each individual Vehicle component on the policy
         </xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="tns:Component">
            <xs:sequence>
               <xs:element minOccurs="0" name="VehicleCoverageType" type="tns:VehicleCoverageTypeEnum">
                  <xs:annotation>
                     <xs:documentation>
Vehicle Coverage Description (Major Peril Description)

Valid Values: Comprehensive|TPFT|TPPD|Contents|Fleet|AsPerSchedule|Caravan|CaravanAnnexe|Contents|MarineHull
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="Excess" type="tns:VehicleExcess">
                  <xs:annotation>
                     <xs:documentation>
Describes the excesses applicable at the VehicleComponent level
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="Driver" type="tns:Driver">
                  <xs:annotation>
                     <xs:documentation>
Holds the driver details for the vehicle component
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="Vehicle" type="tns:Vehicle">
                  <xs:annotation>
                     <xs:documentation>
Holds all vehicles for the component
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
