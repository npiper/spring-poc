        <xs:complexType name="StructuredAddressDetailsSearchCriteria">
      <xs:annotation>
         <xs:documentation>
            Describes the structured address
         </xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="tns:AddressDetailsSearchCriteria">
            <xs:sequence>
               <xs:element minOccurs="0" name="FlatUnitNumber" type="xs:string">
                  <xs:annotation>
                     <xs:documentation>
                        Flat or Unit number
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="BuildingPropertyName1" type="xs:string">
                  <xs:annotation>
                     <xs:documentation>
                        Building name 1
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="HouseNumber1" type="xs:string">
                  <xs:annotation>
                     <xs:documentation>
                        House number 1
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="StreetName" type="xs:string">
                  <xs:annotation>
                     <xs:documentation>
                        The address street name.
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="StreetType" type="xs:string">
                  <xs:annotation>
                     <xs:documentation>
                        The address street type, eg. |Street|Road|Avenue|Place|Close, etc..
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="SuburbPlaceLocality" type="xs:string">
                  <xs:annotation>
                     <xs:documentation>
                        The address suburb.
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="StateTerritory" type="xs:string">
                  <xs:annotation>
                     <xs:documentation>
                        The address state.
                        Valid Values: |Vic|SA|NT|ACT|QLD|TAS|NSW|WA
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="Country" type="xs:string">
                  <xs:annotation>
                     <xs:documentation>
                        The address country name, eg. Australia
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="Postcode" type="xs:string">
                  <xs:annotation>
                     <xs:documentation>
                        The address postcode.
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="DeliveryPoint" type="xs:string">
                  <xs:annotation>
                     <xs:documentation>
                        The unique delivery point ID per address.
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
      <xs:simpleType name="PropertyNoClaimBonusEnum">
