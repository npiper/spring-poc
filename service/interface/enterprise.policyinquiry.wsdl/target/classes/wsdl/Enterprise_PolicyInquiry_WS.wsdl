            <xs:sequence>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="Excess" type="tns:LiabilityExcess"/>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="NoClaimBonus" type="tns:PropertyNoClaimBonus"/>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="Coverage" type="tns:LiabilityCoverage">
                  <xs:annotation>
                     <xs:documentation>
Coverage details for the Liability Component
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="CoverageExtensions" type="tns:LiabilityCoverageExtensions"/>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="Indicator" type="tns:LiabilityIndicator"/>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
