            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
      <xs:complexType name="RiskIdentifier">
      <xs:sequence>
         <xs:element minOccurs="0" name="Identifier">
            <xs:annotation>
               <xs:documentation>
Valid Values:
VehicleRegistration|VIN
               </xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:enumeration value="RiskAddress"/>
                  <xs:enumeration value="VehicleRegistration"/>
