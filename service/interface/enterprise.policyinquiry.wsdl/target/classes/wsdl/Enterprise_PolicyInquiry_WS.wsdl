                     <xs:documentation>
                        Valid values: |BeginsWith|ContainsWord|ExactMatch
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="Name" type="xs:string">
                  <xs:annotation>
                     <xs:documentation>
                        The party name
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="Type" type="tns:OrganisationNameTypeEnum">
                  <xs:annotation>
                     <xs:documentation>
                        Valid Values:
                        Main|TradingName|Other|Registered
