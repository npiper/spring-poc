         <xs:extension base="tns:Excess">
            <xs:sequence>
               <xs:element minOccurs="0" name="Type" type="tns:PolicyExcessTypeEnum">
                  <xs:annotation>
                     <xs:documentation>
Basic |Imposed |Age| Compulsory |Special|Other
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
      <xs:complexType name="Tax">
      <xs:sequence>
         <xs:element minOccurs="0" name="TaxType" type="xs:string">
