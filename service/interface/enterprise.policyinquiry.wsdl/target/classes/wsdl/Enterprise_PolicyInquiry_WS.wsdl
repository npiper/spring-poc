         <xs:enumeration value="Branch"/>
         <xs:enumeration value="Insured"/>
         <xs:enumeration value="Intermediary"/>
         <xs:enumeration value="IntermediaryContact"/>
         <xs:enumeration value="JointPolicyholder"/>
         <xs:enumeration value="ParentOrganisation"/>
         <xs:enumeration value="Policyholder"/>
         <xs:enumeration value="PreviousInsurer"/>
         <xs:enumeration value="Underwriter"/>
      </xs:restriction>
